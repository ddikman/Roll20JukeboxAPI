{
  "manifest_version": 2,

  "name": "Roll20.net Jukebox API",
  "description": "This extensions allow you to control the Roll20 Jukebox through chat messages",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "matches": ["https://app.roll20.net/editor/*"],
      "js": ["jquery.min.js", "api.js"],
      "run_at": "document_end"
    }
  ]
}